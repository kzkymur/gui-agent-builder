{
  "name": "llm-flow-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/engine/__generated__/backend.ts",
    "lint": "biome check .",
    "format": "biome format .",
    "fix": "biome check --apply . && biome format --write ."
  },
  "dependencies": {
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/themes": "^3.0.4",
    "openapi-fetch": "^0.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reactflow": "^11.10.1",
    "sql.js": "^1.9.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@biomejs/biome": "^1.8.3",
    "openapi-typescript": "^7.10.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kzkymur/gui-agent-builder.git"
  },
